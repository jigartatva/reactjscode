    "deploy:dev": "npm run build:dev && cd development && firebase -P firebase-xxxxx deploy",
    "build-win:dev": "set NODE_ENV=development && webpack --progress --colors",
    "build-win:prod": "set NODE_ENV=production && webpack --progress --colors",
    "deploy-win:prod": "cd production && firebase -P firebase-pro-xxxx deploy",
    "deploy-win:dev": "cd development && firebase -P firebase-xxxxx deploy",
    "deploy-win:sync": "cd development && firebase -P firebase-sync-xxxx deploy",
    "start-win": "webpack-dev-server --history-api-fallback --hot --inline --progress --colors --port 3001",
    "start-win:dev": "set NODE_ENV=development && node build-server.js",
    "start-win:prod": "set NODE_ENV=production && node build-server.js",
    "build": "npm run build:prod",
    "build:raw": "webpack --progress --colors"
